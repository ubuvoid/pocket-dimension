{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Envelope",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "type": {
      "type": "string",
      "enum": [
        "UNKNOWN",
        "GREETING"
      ],
      "default": "GREETING"
    },
    "name": {
      "type": "string",
      "default": "test"
    },
    "flag": {
      "type": "boolean",
      "default": true
    },
    "weight": {
      "type": "number",
      "default": 1.5
    },
    "id": {
      "type": "integer",
      "default": 1
    }
  }
}
